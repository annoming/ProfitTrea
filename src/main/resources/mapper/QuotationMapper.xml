<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atming.mapper.QuotationMapper">
    <select id="getNews" resultType="com.atming.entity.News">
        select * from news;
    </select>

    <select id="getAnnounce" resultType="com.atming.entity.Announce">
       select * from announce ORDER BY id LIMIT 0,10
    </select>
</mapper>
